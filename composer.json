{
    "name": "twig/cssinliner-extension",
    "type": "library",
    "description": "A Twig extension to allow inlining CSS",
    "keywords": ["twig", "css", "inlining"],
    "homepage": "https://twig.symfony.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabien Potencier",
            "email": "fabien@symfony.com",
            "homepage": "http://fabien.potencier.org",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "tijsverkoyen/css-to-inline-styles": "^2.0",
        "twig/twig": "^1.0 || ^2.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.1.8"
    },
    "autoload": {
        "psr-4" : {
            "Twig\\CssInliner\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4" : {
            "Twig\\CssInliner\\Tests\\" : "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
